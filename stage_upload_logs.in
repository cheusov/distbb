set -e
. @bindir@/pipestatus

# usage: stage_upload_logs BUILD_ID
test $# -eq 1

# init
BUILD_ID="$1"

DISTBB_CONF=${DISTBB_CONF:-@sysconfdir@/distbb.conf}
. "$DISTBB_CONF"
. @libexecdir@/common

# main
verbose_print "Uploading logs..."
eval $UPLOAD_LOGS_PROG $REPORT1_DIR $UPLOAD_LOGS_DEST
verbose_print "done\n"
