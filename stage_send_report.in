set -e

# usage: stage_upload_pkgs BUILD_ID
test $# -eq 1

# init
BUILD_ID="$1"

: ${DISTBB_CONF:=@sysconfdir@/distbb.conf}
. "$DISTBB_CONF"
. @libexecdir@/common

# main
verbose_print "  sending report... "
eval @libexecdir@/wrapper $REPORT_CMD < "$REPORT1_DIR/META/$REPORT_FILE"
verbose_print "done\n"
